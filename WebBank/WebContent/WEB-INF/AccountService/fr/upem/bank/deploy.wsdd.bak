<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AccountService WSDL service -->

  <service name="Account" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://bank.upem.fr"/>
      <parameter name="wsdlServiceElement" value="AccountService"/>
      <parameter name="schemaQualified" value="http://bank.upem.fr"/>
      <parameter name="wsdlServicePort" value="Account"/>
      <parameter name="className" value="fr.upem.bank.AccountSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Account"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="convert" qname="operNS:convert" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:convertReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:cur1" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:cur2" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:amount" xmlns:pns="http://bank.upem.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getFirstName" qname="operNS:getFirstName" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getFirstNameReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getMail" qname="operNS:getMail" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getMailReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="setMail" qname="operNS:setMail" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:mail" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="setFirstName" qname="operNS:setFirstName" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:firstName" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="setAdress" qname="operNS:setAdress" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:adress" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="setLastName" qname="operNS:setLastName" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:lastName" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getPhoneNumber" qname="operNS:getPhoneNumber" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getPhoneNumberReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="setPhoneNumber" qname="operNS:setPhoneNumber" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:phoneNumber" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="deposit" qname="operNS:deposit" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:depositReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:amount" xmlns:pns="http://bank.upem.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="getAdress" qname="operNS:getAdress" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getAdressReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getLastName" qname="operNS:getLastName" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getLastNameReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getBalance" qname="operNS:getBalance" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getBalanceReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getPassword" qname="operNS:getPassword" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getPasswordReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="getCurrency" qname="operNS:getCurrency" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:getCurrencyReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <operation name="setCurrency" qname="operNS:setCurrency" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:currency" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="withdraw" qname="operNS:withdraw" xmlns:operNS="http://bank.upem.fr" returnQName="retNS:withdrawReturn" xmlns:retNS="http://bank.upem.fr" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:amount" xmlns:pns="http://bank.upem.fr" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="setPassword" qname="operNS:setPassword" xmlns:operNS="http://bank.upem.fr" soapAction="" >
        <parameter qname="pns:password" xmlns:pns="http://bank.upem.fr" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getCurrency setPassword convert getLastName setCurrency getMail setAdress setLastName getPassword getBalance setMail setFirstName setPhoneNumber deposit getPhoneNumber getFirstName getAdress withdraw"/>

  </service>
</deployment>
